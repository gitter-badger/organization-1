{
	"name": "flipboxfactory/organization",
	"description": "Simple parent + child management",
	"version": "1.0.0-beta.10",
	"type": "craft-plugin",
	"minimum-stability": "beta",
	"prefer-stable": true,
	"config": {
		"optimize-autoloader": true
	},
	"keywords": [
		"flipbox",
		"organization",
		"parent-child",
		"craftcms",
		"yii2"
	],
	"license": "MIT",
	"authors": [{
		"name": "Flipbox Factory",
		"email": "hello@flipboxfactory.com"
	}],
	"support": {
		"email": "support@flipboxfactory.com",
		"issues": "https://github.com/flipboxfactory/organization/issues?state=open",
		"source": "https://github.com/flipboxfactory/organization",
		"docs": "https://github.com/flipboxfactory/organization",
		"rss": "https://github.com/flipboxfactory/organization/commits/master.atom"
	},
	"require": {
		"craftcms/cms": "~3.0.0-beta.28",
		"flipboxfactory/craft-spark": "~1.0.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.0",
		"squizlabs/php_codesniffer": "^2.0",
		"codeception/codeception": "^2.2"
	},
	"autoload": {
		"psr-4": {
			"flipbox\\organization\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"flipbox\\organization\\tests\\": "tests/src/"
		}
	},
	"extra": {
		"name": "Organization",
		"handle": "organization",
		"developer": "Flipbox Digital",
		"developerUrl": "https://www.flipboxfactory.com",
		"class": "flipbox\\organization\\Organization",
		"changelogUrl": "https://raw.githubusercontent.com/flipboxfactory/organization/master/CHANGELOG.md",
		"downloadUrl": "https://github.com/flipboxfactory/organization/archive/master.zip",
		"hasCpSettings": true,
		"hasCpSection": true,
		"schemaVersion": "1.0.0",
		"components": {
			"field": "flipbox\\organization\\services\\Field",
			"organization": "flipbox\\organization\\services\\Organization",
			"permission": "flipbox\\organization\\services\\Permission",
			"type": "flipbox\\organization\\services\\Type",
			"user": "flipbox\\organization\\services\\User"
		},
		"modules": {
			"configuration": {
				"basePath": "@flipbox/organization/modules/configuration",
				"class": "flipbox\\organization\\modules\\configuration\\Module",
				"components": {
					"general": "flipbox\\organization\\modules\\configuration\\services\\General",
					"layout": "flipbox\\organization\\modules\\configuration\\services\\Layout",
					"type": "flipbox\\organization\\modules\\configuration\\services\\Type"
				}
			}
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://packagist.org"
		},{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
